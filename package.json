{
  "name": "@ctux/vitejs-react-ts-template",
  "version": "0.5.2",
  "homepage": "https://github.com/cTux/vitejs-react-ts-template",
  "license": "MIT",
  "scripts": {
    "build": "vite build | gnomon",
    "deploy": "gh-pages -d dist | gnomon",
    "dev": "vite",
    "lint": "eslint . | gnomon",
    "prepare": "husky install",
    "preview": "vite preview --port 3093",
    "serve": "vite serve",
    "template:check:dependencies": "pnpm exec depcheck",
    "template:branches:show": "pnpm exec git-removed-branches",
    "template:branches:delete": "pnpm exec git-removed-branches --prune",
    "test": "pnpm run test:vitest && pnpm run test:playwright",
    "test:playwright": "pnpm exec playwright test playwright | gnomon",
    "test:vitest": "vitest --run | gnomon",
    "test:vitest:coverage": "vitest --run --coverage | gnomon",
    "test:vitest:watch": "vitest"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^16.18.65",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/sinon": "^10.0.20",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "@vitejs/plugin-react": "^2.2.0",
    "c8": "^7.14.0",
    "concurrently": "^7.6.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "git-removed-branches": "^2.1.0",
    "gnomon": "^1.5.0",
    "happy-dom": "^7.8.1",
    "husky": "^8.0.3",
    "node-fetch": "^3.3.2",
    "npm-check-updates": "^16.14.11",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "update-template": "^1.3.1",
    "vite": "^3.2.7",
    "vite-plugin-pwa": "^0.17.2",
    "vitest": "^0.34.6"
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2",
      "async@<2.6.4": ">=2.6.4",
      "trim@<0.0.3": ">=0.0.3",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "ejs@<3.1.7": ">=3.1.7",
      "async@>=2.0.0 <2.6.4": ">=2.6.4",
      "got@<11.8.5": ">=11.8.5",
      "terser@>=5.0.0 <5.14.2": ">=5.14.2",
      "minimatch@<3.0.5": ">=3.0.5"
    }
  }
}
